    "# Python program to print positive Numbers in a List\n",
    "\n",
    "# list of numbers\n",
    "list1=[12, 14, -95, 3]\n",
    "num = 0\n",
    "\n",
    "# using while loop\t\n",
    "while(num < len(list1)):\n",
    "\t\n",
    "\t# checking condition\n",
    "\tif list1[num] >= 0:\n",
    "\t\tprint(list1[num], end = \" \")\n",
    "\t\n",
    "\t# increment num\n",
    "\tnum += 1\n",
    "\t\n"
   ]
